{
    "name": "skunenieki/system",
    "description": "Skunenieku Kauss result system",
    "keywords": ["skunenieki"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "ext-mbstring": "*",
        "laravel/lumen-framework": "5.4.*",
        "vlucas/phpdotenv": "^2.2",
        "firebase/php-jwt": "^2.0",
        "league/csv": "^7.1",
        "dg/mysql-dump": "^1.3",
        "guzzlehttp/guzzle": "^6.0",
        "jdorn/sql-formatter": "^1.2",
        "doctrine/dbal": "^2.4",
        "knplabs/knp-snappy": "^0.4.0",
        "h4cc/wkhtmltopdf-amd64": "^0.12",
        "algolia/algoliasearch-client-php": "1.5.*",
        "arubacao/http-basic-auth-guard": "^1.0"
    },
    "require-dev": {
        "heroku/heroku-buildpack-php": "*",
        "phpunit/phpunit": "^5.0",
        "phpspec/phpspec": "^2.1",
        "squizlabs/php_codesniffer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Skunenieki\\System\\": "app/"
        },
        "classmap": [
            "database"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Skunenieki\\System\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ],
        "compile": [
            "php artisan migrate --force"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true
}
